{
	"compilerOptions": {
		"outDir": "./dist",
		"rootDir": "./",
		"target": "es2022",
		"lib": ["es2023"],
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"declaration": true,
		"declarationMap": true,
		"strictNullChecks": true
	},
	"include": ["./**/*.ts"]
}
